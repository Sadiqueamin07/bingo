pipeline {
    agent { label 'agentlinux' }
    stages {
        stage('Prepare Content') {
            steps {
                sh 'echo "<h1>Hello from Jenkins</h1>" > index.html'
            }
        }
        stage('Deploy to S3') {
            steps {
                withAWS(credentials: 'aws-credentials', region: 'ap-south-1') {
                    s3Upload(file: 'index.html', bucket: 'mycontentnode.js', path: 'website/')
                }
            }
        }
    }
}
