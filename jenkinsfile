pipeline {
    agent any
    stages {
        stage('Build with Version') {
            steps {
                sh "java --version"
                sh "ls || dir" // Handles both Linux (ls) and Windows (dir)
            }
        }
        stage('Deploy') {
            steps {
                sh "aws s3 cp Tiger.jpg s3://my-image011/"
            }
        }
        stage('Invalidate CloudFront Cache') {
            steps {
                withAWS(credentials: 'aws-credentials', region: 'ap-south-1') {
                sh "aws cloudfront create-invalidation --distribution-id d1xb1okcfbzsa1.cloudfront.net --paths '/*'"
        }
    }
}
    }
}
